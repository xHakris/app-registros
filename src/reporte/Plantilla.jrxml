	<queryString language="SQL">
		<![CDATA[SELECT
     materia.`nombre` AS materia_nombre,
     reporte.`id` AS reporte_id,
     horarios.`horaEntrada` AS horarios_horaEntrada,
     horarios.`horaSalida` AS horarios_horaSalida,
     docentes.`nombre` AS docentes_nombre,
     reporte.`id_Horario` AS reporte_id_Horario
FROM
     `horarios` horarios INNER JOIN `reporte` reporte ON horarios.`id` = reporte.`id_Horario`
     INNER JOIN `laboratorios` laboratorios ON reporte.`id_Laboratorio` = laboratorios.`id`
     INNER JOIN `materia` materia ON reporte.`id_Materia` = materia.`id`
     INNER JOIN `periodoacademico` periodoacademico ON reporte.`id_periodo` = periodoacademico.`id`
     INNER JOIN `docentes` docentes ON materia.`docenteID` = docentes.`id`
     INNER JOIN `carrera` carrera ON materia.`carreraID` = carrera.`id`,
     `facultades` facultades]]>
	</queryString>
